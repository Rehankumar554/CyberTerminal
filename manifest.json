{
  "manifest_version": 3,
  "name": "Cyber Terminal - Linux Shell",
  "version": "1.1.0",
  "description": "Level 7 Linux-grade Cyber Terminal for Chrome New Tab",
  "permissions": ["storage"],

  "icons": {
    "16": "assets/image/icon.png",
    "32": "assets/image/icon.png",
    "48": "assets/image/icon.png",
    "128": "assets/image/icon.png"
  },

  "action": {
    "default_icon": {
      "16": "assets/image/icon.png",
      "32": "assets/image/icon.png",
      "48": "assets/image/icon.png",
      "128": "assets/image/icon.png"
    }
  },

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
